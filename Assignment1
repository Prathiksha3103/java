package assignment;

public class Device {
	
	   
	    private String status;

	    
	    public Device(String status) {
	        this.status = status;
	    }

	   
	    public String getStatus() {
	        return status;
	    }

	    
	    public void setStatus(String status) {
	        this.status = status;
	    }
}



