
TC 1,2
package oops; 
import java.util.Scanner; 
class Shape { 
double area() { 
System.out.println("Default area method of Shape"); 
return 0.0; 
} 
}
 
class Circle extends Shape { 
double radius; 
Circle(double radius) { 
this.radius = radius; 
} 
@Override 
double area() { 
return Math.PI * radius * radius; 
} 
public class User { 
public static void main(String[] args) { 
Scanner sc = new Scanner(System.in); 
System.out.println("Prathiksha S"); 
System.out.println("2117240070229"); 

 
System.out.print("Enter radius of circle: "); 
double r = sc.nextDouble(); 
Circle c = new Circle(r); 
System.out.println("Circle area: " + c.area()); 
sc.close(); 
} 
}

TC 3,4:
package oops; 
import java.util.Scanner; 
class Shape { 
double area() { 
System.out.println("Default area method of Shape"); 
return 0.0; 
} 
} 
class Rectangle extends Shape { 
double length, width; 
Rectangle(double length, double width) { 
this.length = length; 
this.width = width; 
} 
@Override 
double area() { 
return length * width; 
} 
} 
public class User { 

public static void main(String[] args) { 
Scanner sc = new Scanner(System.in); 
System.out.println("Prathiksha S"); 
System.out.println("2117240070229"); 
 
System.out.print("Enter length of rectangle: "); 
double l = sc.nextDouble(); 
System.out.print("Enter width of rectangle: "); 
double w = sc.nextDouble(); 
Rectangle rect = new Rectangle(l, w); 
System.out.println("Rectangle area: " + rect.area()); 
sc.close(); 
}
TC 5:
package oops; 
import java.util.Scanner; 
class Shape { 
double area() { 
System.out.println("Default area method of Shape"); 
return 0.0; 
} 

} 
public class User { 
public static void main(String[] args) { 
Scanner sc = new Scanner(System.in); 
System.out.println("Priyadharshini K"); 
System.out.println("2117240070233"); 
Shape s = new Shape(); 
System.out.println("Shape area: "+s.area()); 
sc.close(); 
} 
}


